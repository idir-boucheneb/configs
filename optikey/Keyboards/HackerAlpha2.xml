<Keyboard>
    <Name>HackerAlpha2</Name>
    <WindowState>Docked</WindowState>
    <DockSize>Collapsed</DockSize>
    <Position>Bottom</Position>
    <Width>90%</Width>
    <Height>37%</Height>
    <BackgroundColor>Transparent</BackgroundColor>
    <BorderColor>Transparent</BorderColor>
    <SymbolMargin>20</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
    <Grid>
        <Rows>4</Rows>
        <Cols>18</Cols>
    </Grid>
    <Content>
        <!-- First row -->
        <DynamicKey Row="0" Col="0">
            <Label>(</Label>
            <Text>(</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Action>Escape</Action>
            <Label>Esc</Label>
        </DynamicKey>
        <DynamicKey>
            <Label>a</Label>
            <Text>a</Text>
            <ShiftDownLabel>A</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>z</Label>
            <Text>z</Text>
            <ShiftDownLabel>Z</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>e</Label>
            <Text>e</Text>
            <ShiftDownLabel>E</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>r</Label>
            <Text>r</Text>
            <ShiftDownLabel>R</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>t</Label>
            <Text>t</Text>
            <ShiftDownLabel>T</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>y</Label>
            <Text>y</Text>
            <ShiftDownLabel>Y</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>u</Label>
            <Text>u</Text>
            <ShiftDownLabel>U</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>i</Label>
            <Text>i</Text>
            <ShiftDownLabel>I</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>o</Label>
            <Text>o</Text>
            <ShiftDownLabel>O</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>p</Label>
            <Text>p</Text>
            <ShiftDownLabel>P</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>'</Label>
            <Text>'</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>=</Label>
            <Text>=</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Action>BackOne</Action>
            <Symbol>BackOneIcon</Symbol>
        </DynamicKey>
        <DynamicKey>
            <Label>)</Label>
            <Text>)</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>


        <!-- Second row -->
        <DynamicKey Row="1" Col="0">
            <Label>{</Label>
            <Text>{</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Symbol>TabIcon</Symbol>
            <Text>&#9;</Text>
        </DynamicKey>
        <DynamicKey>
            <Label>q</Label>
            <Text>q</Text>
            <ShiftDownLabel>Q</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>s</Label>
            <Text>s</Text>
            <ShiftDownLabel>S</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>d</Label>
            <Text>d</Text>
            <ShiftDownLabel>D</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>f</Label>
            <Text>f</Text>
            <ShiftDownLabel>F</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>g</Label>
            <Text>g</Text>
            <ShiftDownLabel>G</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>h</Label>
            <Text>h</Text>
            <ShiftDownLabel>H</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>j</Label>
            <Text>j</Text>
            <ShiftDownLabel>J</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>k</Label>
            <Text>k</Text>
            <ShiftDownLabel>K</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>l</Label>
            <Text>l</Text>
            <ShiftDownLabel>L</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>m</Label>
            <Text>m</Text>
            <ShiftDownLabel>M</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>_</Label>
            <Text>_</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>$</Label>
            <Text>$</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Symbol>EnterIcon</Symbol>
            <Text>&#13;</Text>
        </DynamicKey>
        <DynamicKey>
            <Label>}</Label>
            <Text>}</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>


        <!-- Third row -->
        <DynamicKey Row="2" Col="0">
            <Label>[</Label>
            <Text>[</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Symbol>ShiftIcon</Symbol>
            <Action>LeftShift</Action>
        </DynamicKey>
        <DynamicKey>
            <Label>w</Label>
            <Text>w</Text>
            <ShiftDownLabel>W</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>x</Label>
            <Text>x</Text>
            <ShiftDownLabel>X</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>c</Label>
            <Text>c</Text>
            <ShiftDownLabel>C</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>v</Label>
            <Text>v</Text>
            <ShiftDownLabel>V</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>b</Label>
            <Text>b</Text>
            <ShiftDownLabel>B</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>n</Label>
            <Text>n</Text>
            <ShiftDownLabel>N</ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>.</Label>
            <Text>.</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>,</Label>
            <Text>,</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>;</Label>
            <Text>;</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>-&#x3E;</Label>
            <Text>-&#x3E;</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>:</Label>
            <Text>:</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>"</Label>
            <ShiftDownLabel></ShiftDownLabel>
            <Text>"</Text>
        </DynamicKey>
        <DynamicKey>
            <Action>Sleep</Action>
            <Symbol>SleepIcon</Symbol>
        </DynamicKey>
        <DynamicKey>
            <Symbol>MinimiseIcon</Symbol>
            <Action>Minimise</Action>
        </DynamicKey>
        <DynamicKey>
            <Label>]</Label>
            <Text>]</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>


        <!-- Fourth row -->
        <DynamicKey Row="3" Col="0">
            <Label>&#x3C;</Label>
            <Text>&#x3C;</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Action>LeftCtrl</Action>
            <Label>Ctrl</Label>
        </DynamicKey>
        <DynamicKey>
            <Label>-</Label>
            <Text>-</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>/</Label>
            <Text>/</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>             
	<DynamicKey>
            <Label>!</Label>
            <Text>!</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>|</Label>
            <Text>|</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="4">
            <Symbol>SpaceIcon</Symbol>
            <Text>&#32;</Text>
        </DynamicKey>
        <DynamicKey>
            <Label>&#x26;</Label>
            <Text>&#x26;</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>?</Label>
            <Text>?</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>*</Label>
            <Text>*</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey>
            <Label>+</Label>
            <Text>+</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
        <DynamicKey Width="2">
            <Symbol>NumericAndSymbolsIcon</Symbol>
            <ChangeKeyboard BackReturnsHere="True">HackerNum2</ChangeKeyboard>
        </DynamicKey>
        <DynamicKey>
            <Label>&#x3E;</Label>
            <Text>&#x3E;</Text>
            <ShiftDownLabel></ShiftDownLabel>
        </DynamicKey>
    </Content>
</Keyboard>
