<Keyboard>
    <Name>Vim</Name>
    <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
    <WindowState>Docked</WindowState>
    <DockSize>Collapsed</DockSize>
    <Position>Bottom</Position>
    <BackgroundColor>#000000</BackgroundColor>
    <BorderColor>#000000</BorderColor>
    <SymbolMargin>10</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>

    <KeyGroup Name="backone" LockOnTime="100" CompletionTimes="250,400,200" BackgroundColor="#FF0000"/>
    <KeyGroup Name="abc" BackgroundColor="#FFFFFF" KeyDisabledBackground="gray" AutoScaleToOneKeyWidth="false"/>
    <KeyGroup Name="123" BackgroundColor="#7070AA" AutoScaleToOneKeyWidth="false"/>
    <KeyGroup Name="symbols" BackgroundColor="#808080" AutoScaleToOneKeyWidth="false" />
    <KeyGroup Name="modifiers" BackgroundColor="#0000FF" AutoScaleToOneKeyWidth="false"/>
    <KeyGroup Name="functions1" BackgroundColor="#D0D0D0" AutoScaleToOneKeyWidth="false"/>
    <KeyGroup Name="functions2" BackgroundColor="#FFFF00" AutoScaleToOneKeyWidth="false"/>
    <KeyGroup Name="arrows" BackgroundColor="#907090" AutoScaleToOneKeyWidth="false"/>

    <Grid>
        <Rows>5</Rows>
        <Cols>168</Cols>
    </Grid>
    <Content>
        <!-- First row -->
        <DynamicKey Row="0" Col="0" Width="10">
            <KeyGroup>functions1</KeyGroup>
            <Action>Delete</Action>
            <Label>Del</Label>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions2</KeyGroup>
            <Action>Escape</Action>
            <Label>Esc</Label>
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8"></DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Symbol>EndIcon</Symbol>
            <KeyDown>LeftCtrl</KeyDown>
            <KeyDown>f</KeyDown>
            <KeyUp>f</KeyUp>
            <KeyUp>LeftCtrl</KeyUp>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Label>˅</Label>
            <KeyDown>LeftCtrl</KeyDown>
            <KeyDown>d</KeyDown>
            <KeyUp>d</KeyUp>
            <KeyUp>LeftCtrl</KeyUp>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Label>↓</Label>
            <Text>5</Text>
            <Text>j</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>UpArrowKeyIcon</Symbol>
            <Text>k</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Label>↑</Label>
            <Text>5</Text>
            <Text>k</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Label>˄</Label>
            <KeyDown>LeftCtrl</KeyDown>
            <KeyDown>u</KeyDown>
            <KeyUp>u</KeyUp>
            <KeyUp>LeftCtrl</KeyUp>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Symbol>HomeIcon</Symbol>
            <KeyDown>LeftCtrl</KeyDown>
            <KeyDown>b</KeyDown>
            <KeyUp>b</KeyUp>
            <KeyUp>LeftCtrl</KeyUp>
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8"></DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>backone</KeyGroup>
            <Action>BackOne</Action>
            <Symbol>BackOneIcon</Symbol>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
            <ShiftDownLabel></ShiftDownLabel>
            <Symbol>BackIcon</Symbol>
            <Action>BackFromKeyboard</Action>
        </DynamicKey>


        <!-- Second row -->
        <DynamicKey Row="1" Col="0" Width="10">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>DownArrowKeyIcon</Symbol>
            <Action>ArrowDown</Action>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>TabIcon</Symbol>
            <Text>&#9;</Text>
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Symbol>ArrowPointingToLeftIcon</Symbol>
            <Text>_</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>LeftArrowKeyIcon</Symbol>
            <Text>l</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>DownArrowKeyIcon</Symbol>
            <Text>j</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>RightArrowKeyIcon</Symbol>
            <Text>m</Text>
        </DynamicKey>
        <DynamicKey Width="8">
            <KeyGroup>arrows2</KeyGroup>
            <Symbol>ArrowPointingToRightIcon</Symbol>
            <Text>$</Text>
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>EnterIcon</Symbol>
            <Text>&#13;</Text>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>UpArrowKeyIcon</Symbol>
            <Action>ArrowUp</Action>
        </DynamicKey>


        <!-- Third row -->
        <DynamicKey Row="2" Col="0" Width="10">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>LeftArrowKeyIcon</Symbol>
            <Action>ArrowLeft</Action>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>ShiftIcon</Symbol>
            <Action>LeftShift</Action>
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>ShiftIcon</Symbol>
            <Action>LeftShift</Action>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
            <KeyGroup>arrows</KeyGroup>
            <Symbol>RightArrowKeyIcon</Symbol>
            <Action>ArrowRight</Action>
        </DynamicKey>


        <!-- Fourth row -->
        <DynamicKey Row="3" Col="0" Width="10">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>WinIcon</Symbol>
            <Action>LeftWin</Action>
        </DynamicKey>
        <DynamicKey Width="10">
            <KeyGroup>arrows</KeyGroup>
            <Action>MouseMoveAndLeftClick</Action>
            <Symbol>MouseLeftClickIcon</Symbol>
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Action>LeftCtrl</Action>
            <Label>Ctrl</Label>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="24">
            <KeyGroup>functions1</KeyGroup>
            <Symbol>SpaceIcon</Symbol>
            <Text>&#32;</Text>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="12">
            <KeyGroup>functions1</KeyGroup>
            <Action>LeftCtrl</Action>
            <Label>Ctrl</Label>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
            <KeyGroup>functions1</KeyGroup>
            <Label>Alt</Label>
            <Action>LeftAlt</Action>
        </DynamicKey>






        






        <DynamicKey Row="4" Col="0" Width="10">
            <KeyGroup>modifiers</KeyGroup>
            <Action>Sleep</Action>
            <Symbol>SleepIcon</Symbol>
        </DynamicKey>
        <DynamicKey Width="10">
	    <KeyGroup>arrows</KeyGroup>
            <Action>MouseMoveAndLeftDoubleClick</Action>
            <Symbol>MouseLeftDoubleClickIcon</Symbol>
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="8">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
        </DynamicKey>
        <DynamicKey Width="10">
            <KeyGroup>modifiers</KeyGroup>
            <Label>Switcher</Label>
            <Plugin>
                <Name>ExternalProgram</Name>
                <Method>RUN</Method>
                <Argument>
                    <Name>command</Name>
                    <Value>"C:\Program Files\Tobii Dynavox\Switcher\Tdx.Switcher.exe"</Value>
                </Argument>
                <Argument>
                    <Name>parameters</Name>
                    <Value>-s</Value>
                </Argument>
            </Plugin>
        </DynamicKey>
    </Content>
</Keyboard>
